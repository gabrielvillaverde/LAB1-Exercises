#include <stdio.h>
#include <stdlib.h>

int main(void) {
	float resultado;
	int numero1;
	int numero2;
	int respuesta;

	printf("\nIngrese el número 1: ");
	scanf("%d", &numero1);

	printf("\nIngrese el número 2: ");
	scanf("%d", &numero2);

	respuesta = dividir(&resultado,numero1,numero 2);

	return EXIT_SUCCESS;
}
